{
  "name": "rag-service",
  "version": "1.0.0",
  "description": "RAG Service for Document Chunking, Embedding and Retrieval",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "mkdir -p dist/agents/prompts && cp -r src/agents/prompts/*.md dist/agents/prompts/",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@langchain/community": "^0.3.17",
    "@langchain/core": "^0.3.17",
    "@langchain/google-genai": "^0.1.4",
    "@xenova/transformers": "^2.17.1",
    "axios": "^1.12.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "langchain": "^0.3.7",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
